<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>MultiPageQuizApp</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.quiz.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>QuestionServlet</servlet-name>
    <servlet-class>com.quiz.QuestionServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>QuestionServlet</servlet-name>
    <url-pattern>/QuestionServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>AnswerServlet</servlet-name>
    <servlet-class>com.quiz.AnswerServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>AnswerServlet</servlet-name>
    <url-pattern>/AnswerServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ResultServlet</servlet-name>
    <servlet-class>com.quiz.ResultServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ResultServlet</servlet-name>
    <url-pattern>/ResultServlet</url-pattern>
</servlet-mapping>

</web-app>